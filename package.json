    "hapi": "17.5.4",
    "inert": "5.1.0",
    "joi": "13.6.0",
    "nunjucks": "3.1.3",
    "standard": "12.0.1",
    "vision": "5.4.0",
    "yar": "9.0.1"
  },
  "devDependencies": {
    "blipp": "^3.0.0",
    "cheerio": "1.0.0-rc.2",
    "chromedriver": "2.42.0",
    "code": "5.2.0",
