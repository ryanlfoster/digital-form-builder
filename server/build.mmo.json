    {
      "path": "/licence-not-required-exemption-available-article-19",
      "components": [
        {
          "type": "Html",
          "content": "<div class=\"govuk-prose-scope\">Based on the information provided your activity is one that meets the terms of an marine licence exemption set out in Article 19 of the Marine licence (exempted Activities) Order and a marine licence is therefore not required. This exemption does not require any action from you before it is undertaken.</div>"
        }
      ],
      "title": "Licence is not required"
    },
    {
      "path": "/licence-not-required-devolved",
      "components": [
        {
          "type": "Html",
          "content": "<div class=\"govuk-prose-scope\">Based on the information provided the activity does not require a marine licence from the MMO but you may require a marine licence from the relevant devolved administration.</div>"
